    "helmet": "^7.1.0",
    "moment-timezone": "^0.5.43",
    "pg": "^8.11.3",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "lead",
    "operation",
    "management",
    "automation"
  ],
